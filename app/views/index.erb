<h1>Recipes</h1>
  <ul>
    <%@recipes.each do |recipe|%>
      <li><a href="/recipes/<%=recipe.id%>"><%=recipe.name%></a></li>
      <li><a href="/recipes/<%=recipe.id%>"><%=recipe.ingredients%></a></li>
      <li><a href="/recipes/<%=recipe.id%>"><%=recipe.cook_time%></a></li>
    <%end%>
  </ul>
